<?xml version="1.0"?>
<extension key="com.joineryhq.groupreg" type="module">
  <file>groupreg</file>
  <name>Group Event Participant Registration</name>
  <description>Provides event registration features for family and friends, even if registering user (e.g., parent) is not attending.</description>
  <license>GPL-3.0</license>
  <maintainer>
    <author>Allen Shaw</author>
    <email>allen@JoineryHQ.com</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/twomice/com.joineryhq.groupreg/</url>
    <url desc="Documentation">https://twomice.github.io/com.joineryhq.groupreg/</url>
    <url desc="Support">https://github.com/twomice/com.joineryhq.groupreg/issues</url>
    <url desc="Licensing">http://www.gnu.org/licenses/gpl-3.0.html</url>
  </urls>
  <releaseDate>2021-02-05</releaseDate>
  <version>1.1.1</version>
  <develStage>beta</develStage>
  <compatibility>
    <ver>5.0</ver>
  </compatibility>
  <comments>Developed by Joinery, with initial development sponsored by Nameless Network.</comments>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/Groupreg</namespace>
  </civix>
</extension>
